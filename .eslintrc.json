{
  "env": {
      "browser": true,
      "es6": true,
      "jest": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
  },
  "rules": {
      "import/no-extraneous-dependencies": 0,
      "no-unused-expressions": 0,
      "arrow-parens": 0,
      "quotes": 0
  }
}
